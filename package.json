{
  "name": "next-fast",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:client dev:server",
    "dev:client": "next dev",
    "dev:server": "uvicorn main:app --app-dir=server --reload",
    "start": "run-p start:client start:server",
    "start:client": "next build && next start",
    "start:server": "uvicorn main:app --app-dir=server",
    "lint": "next lint",
    "fmt": "prettier --write 'pages/**/*.js' 'styles/**/*.scss' '*.{js, json}'",
    "pip": "pip install -r server/requirements.txt"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "next": "12.1.4",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "sass": "^1.49.11"
  }
}
